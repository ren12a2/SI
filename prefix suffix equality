int pse(char *s, char *t){

    int n = strlen(s), m= strlen(t);

    if(m>n)
    return 0;

    for(int i=0; i<m; i++){
        if(s[i]!= t[i])
        return 0;
    }

    int ss= n-m;
    for(int i=0; i<m; i++){
        if(s[ss+i]!=t[i])
        return 0;
    }

    return 1;

};

